<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

<bean class="com.spring.laptop.exe1.SpringEmployee.Employee1" id="e1">
	<constructor-arg value="11"></constructor-arg>
	<constructor-arg value="Rajesh"></constructor-arg>
	<constructor-arg value="65000F"></constructor-arg>
	
</bean>
<bean class="com.spring.laptop.exe1.SpringEmployee.Employee2" id="e2">
	<property name="e_id" value="12"></property>
	<property name="e_name" value="Ramesh"></property>
	<property name="e_salary" value="55000F"></property>
	</bean>
</beans>