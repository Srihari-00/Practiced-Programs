<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean class="com.spring.laptop.exe1.SpringExample2.Laptop" id="lp">
		<property name="serialNo" value="11"></property>
		<property name="brand" value="Asus"></property>
		<property name="price" value="65000"></property>

	</bean>
	<bean class="com.spring.laptop.exe1.SpringExample2.Laptop"
		id="lp1">
		<constructor-arg value="12"></constructor-arg>
		<constructor-arg value="Dell"></constructor-arg>
		<constructor-arg value="45000"></constructor-arg>
	</bean>
</beans>